#init
mkdir app
cd    app
echo  "#app" >> README.md

git init
git add .
git commit -m "README.md"
git remote add origin git@github.com:takagotch/app.git
git push -u origin master

#clone
git clone git@github.com:takagotch/app.git
#         git@bitbucket.org:takagotch/app.git
cd app
cp -r ~/Desktop/app .
git add .
git commit -m "first commit"
git push -u origin master

